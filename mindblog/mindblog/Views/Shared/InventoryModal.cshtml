<link rel="stylesheet" href="@Url.Content("~/Scripts/windowfiles/dhtmlwindow.css")">
<script type="text/javascript" src="@Url.Content("~/Scripts/windowfiles/dhtmlwindow.js")"></script>

<style>
#wrapper { width: 710px; height: 1420px; padding: 0; overflow: hidden; }
#googlebox { width: 700px; height: 450px; border: 0px; }
#googlebox {
    zoom: 0.90;
    -moz-transform: scale(0.90);
    -moz-transform-origin: 0 0;
    -o-transform: scale(0.90);
    -o-transform-origin: 0 0;
    -webkit-transform: scale(0.90);
    -webkit-transform-origin: 0 0;
}

@@media screen and (-webkit-min-device-pixel-ratio:0) {
 #googlebox  { zoom: 1;  }
}
</style>  

<script>
        (function ($) {
 
            $('[data-showwindow=true]').on('click', function (event) {
                event.preventDefault();

                var googlewin=dhtmlwindow.open("googlebox",
                    "iframe",
                    $(this).attr('data-url'),
                    $(this).attr('data-windowtitle'),
                    "width=700px,height=450px,resize=1,scrolling=1,center=1",
                    "recal");

            });
           
        }(jQuery));
</script>

 